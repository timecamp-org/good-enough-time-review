{
  "manifest_version": 3,
  "name": "Calendar Events Analyzer",
  "version": "1.0",
  "description": "Upload and analyze calendar CSV files with event name normalization",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["templates/*"],
      "matches": ["<all_urls>"]
    }
  ]
} 